{
  "name": "mediamonks/rest-api-bundle",
  "type": "symfony-bundle",
  "description": "MediaMonks Rest API Symfony Bundle",
  "keywords": [
    "rest",
    "api",
    "bundle",
    "symfony"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "mediamonks/rest-api": "^2.2"
  },
  "require-dev": {
    "symfony/symfony": "^4.4|^5.0",
    "sensio/framework-extra-bundle": "^4.4|^5.0",
    "phpunit/phpunit": "^6.0|^7.0|^8.0",
    "mockery/mockery": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^3.0|^4.0"
  },
  "suggest": {
    "jms/serializer-bundle": "Use JMS Serializer as a serializer"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\RestApiBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "Tests/Functional/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  }
}
