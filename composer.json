{
  "name": "mediamonks/rest-api-bundle",
  "type": "symfony-bundle",
  "description": "MediaMonks Rest API Symfony Bundle",
  "keywords": [
    "rest",
    "api",
    "bundle",
    "symfony"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^8.0",
    "mediamonks/rest-api": "^2.3"
  },
  "require-dev": {
    "symfony/symfony": "^5.0||^6.0",
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "matthiasnoback/symfony-dependency-injection-test": "^4.0",
    "monolog/monolog": "^2.8||^3.2",
    "masterminds/html5": "^2.7"
  },
  "suggest": {
    "jms/serializer-bundle": "Use JMS Serializer as a serializer"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\RestApiBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "Tests/Functional/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0-dev"
    }
  }
}
