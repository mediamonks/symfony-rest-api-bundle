{
  "name": "krioo/rest-api-bundle",
  "type": "symfony-bundle",
  "description": "MediaMonks Rest API Bundle",
  "keywords": [
    "rest",
    "api",
    "bundle",
    "symfony"
  ],
  "homepage": "https://www.mediamonks.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Slootjes",
      "email": "robert@mediamonks.com",
      "homepage": "https://github.com/slootjes"
    }
  ],
  "require": {
    "php": "^5.4|~7.0",
    "symfony/dependency-injection": "^2.7|^3.0",
    "symfony/http-kernel": "^2.7|^3.0"
  },
  "require-dev": {
    "symfony/symfony": "^2.7|^3.0",
    "sensio/framework-extra-bundle": "^3.0",
    "codeclimate/php-test-reporter": "dev-master@dev",
    "matthiasnoback/symfony-dependency-injection-test": "0.*",
    "phpunit/phpunit": "^4.8",
    "mockery/mockery": "^0.9.4"
  },
  "suggest": {
    "jms/serializer-bundle": "Use JMS Serializer as a serializer"
  },
  "autoload": {
    "psr-4": {
      "MediaMonks\\RestApiBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-0": {
      "": "Tests/Functional/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
